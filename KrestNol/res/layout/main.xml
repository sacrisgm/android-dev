<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn00"
		android:layout_marginRight="13dp"
		android:layout_toLeftOf="@+id/btn01"
		android:layout_marginTop="65dp"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn01"
		android:layout_alignTop="@+id/btn00"
		android:layout_centerHorizontal="true"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn02"
		android:layout_alignTop="@+id/btn01"
		android:layout_toRightOf="@+id/btn01"
		android:layout_marginLeft="13dp"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn10"
		android:layout_below="@+id/btn00"
		android:layout_alignLeft="@+id/btn00"
		android:layout_marginTop="40dp"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn11"
		android:layout_alignTop="@+id/btn10"
		android:layout_centerHorizontal="true"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn12"
		android:layout_alignTop="@+id/btn11"
		android:layout_alignLeft="@+id/btn02"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn20"
		android:layout_below="@+id/btn10"
		android:layout_alignLeft="@+id/btn10"
		android:layout_marginTop="40dp"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn21"
		android:layout_below="@+id/btn11"
		android:layout_centerHorizontal="true"
		android:layout_alignTop="@+id/btn20"/>

	<Button
		android:layout_width="90dp"
		android:layout_height="90dp"
		android:id="@+id/btn22"
		android:layout_alignTop="@+id/btn21"
		android:layout_alignLeft="@+id/btn12"/>

	<Button
		android:layout_width="120dp"
		android:layout_height="wrap_content"
		android:text="@string/button_reset"
		android:id="@+id/btnRst"
		android:layout_alignParentBottom="true"
		android:layout_centerHorizontal="true"/>

</RelativeLayout>

